<% const links = JSON.parse(include("../../assets/json/nav.json")); %>
<nav id="topNav">
    <div id="logo" class="nav-logo">
        <a href="/" class="india-font">Prince of India</a>
    </div>
    <div class="nav-icon">
        <i class="fas fa-bars"></i>
    </div>
    <ul id="ulNav">
        <% links.navLinks.forEach(link => { %>
            <li><a title="<%= link.title %>" href="<%= link.href %>"><%= link.text %></a></li>
        <% }); %>
    </ul>
</nav>